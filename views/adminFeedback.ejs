<!--%= data %-->
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Feedback From Customers</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css">
    <link rel="stylesheet" href="../css/table.css">

</head>
<nav class="navbar navbar-dark bg-dark py-3">
    <a class="navbar-brand" href="/admin/login"><i class="fas fa-arrow-left"></i> Back</a>
</nav>

<body>
    <!-- partial:index.partial.html -->
    <section>
        <!--for demo wrap-->
        <h1>Feedback From Customers</h1>
        <div class="tbl-header">
            <table cellpadding="0" cellspacing="0" border="0">
                <thead>
                    <tr>
                        <th>Username</th>
                        <th>Rating</th>
                        <th>Message</th>

                        <!--th>Zipcode</th>
                            <th>Email-ID</th>
                            <th>Phone Number</th-->
                    </tr>
                </thead>
            </table>
        </div>
        <div class="tbl-content">
            <table cellpadding="0" cellspacing="0" border="0">
                <tbody>
                    <% for (var i = 0; i < data.length; i++) { %>
                        <tr>
                            <td>
                                <%= data[i].author.username %>
                            </td>
                            <td>
                                <%= data[i].review.rating %>
                            </td>
                            <td>
                                <%= data[i].review.message %>
                            </td>

                        </tr>
                        <% } %>
                </tbody>
            </table>
        </div>
    </section>


    <!-- follow me template -->
    <div class="made-with-love">

        <a target="_blank" href="/">FoodCloud inc </a>
    </div>
    <!-- partial -->
    <script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src="../js/table.js"></script>

</body>

</html>